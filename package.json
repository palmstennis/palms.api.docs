{
  "name": "palms.docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "aglio": "^2.3.0",
    "aglio-theme-onlicar": "^1.6.4",
    "gh-pages": "^2.0.1"
  },
  "devDependencies": {},
  "scripts": {
    "postinstall": "mkdir dist-club && mkdir dist-player",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-club": "aglio -s -i club.apib --theme-fullWidth=true",
    "start-player": "aglio -s -i player.apib --theme-fullWidth=true",
    "build-club": "aglio -i club.apib --theme-fullWidth=true -o dist-club/index.html",
    "release-club": "yarn build-club && yarn deploy-club",
    "deploy-club": "gh-pages -d dist-club",
    "build-player": "aglio -i player.apib --theme-fullWidth=true -o dist-player/index.html",
    "release-player": "yarn build-player && yarn deploy-player",
    "deploy-player": "gh-pages -d dist-player --repo git@github.com:palmstennis/palms.player.docs.git",
    "release": "yarn release-player && yarn release-club"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/palms_/palms.docs.git"
  },
